            @Html.DisplayFor(modelItem => item.Adress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiemToan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiemVan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiemAnh)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HocLuc)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.ID }) |

            @Html.ActionLink("Xóa", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
